    int e=a+a+a+a;
    int f=a+a+a+a+a;
    int g=a+a+a+a+a+a;
    int h=a+a+a+a+a+a+a;
    int i=a+a+a+a+a+a+a+a;
    int j=a+a+a+a+a+a+a+a+a;
    int k=a+a+a+a+a+a+a+a+a+a;
    
    printf("your table is\n");
    
    printf("%d\n",b);
    printf("%d\n",c);
    printf("%d\n",d);
    printf("%d\n",e);
    printf("%d\n",f);
    printf("%d\n",g);
    printf("%d\n",h);
